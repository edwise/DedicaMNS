<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edwise.dedicamns"
    android:versionCode="11"
    android:versionName="1.4" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.edwise.dedicamns.LoginActivity"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.edwise.dedicamns.MonthViewActivity"
            android:label="@string/title_activity_month_view" />
        <activity
            android:name="com.edwise.dedicamns.DetailDayActivity"
            android:label="@string/title_activity_detail_day" >
        </activity>
        <activity
            android:name="com.edwise.dedicamns.MainMenuActivity"
            android:label="@string/title_activity_main_menu" >
        </activity>
        <activity
            android:name="com.edwise.dedicamns.BatchMenuActivity"
            android:label="@string/title_activity_batch_menu" >
        </activity>
        <activity
            android:name="com.edwise.dedicamns.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.edwise.dedicamns.GeneralDetailDayActivity"
            android:label="@string/title_activity_general_detail_day" >
        </activity>
        <activity
            android:name="com.edwise.dedicamns.MonthReportActivity"
            android:label="@string/title_activity_month_report" >
        </activity>
    </application>

</manifest>